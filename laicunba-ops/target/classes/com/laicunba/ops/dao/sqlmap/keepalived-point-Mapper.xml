<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="KEEPALIVED_POINT">

	<resultMap type="com.laicunba.ops.pojo.KeepalivedPointDO" id="map" extends="BASE.map">
		<result column="cn_Name" property="cnName" />
		<result column="level" property="level" />
		<result column="user_Type" property="userType" />
		<result column="interval" property="interval" />
		<result column="owner" property="owner" />
		<result column="type" property="type" />
		<result column="key" property="key" /> 
	</resultMap>

	<select id="list" resultMap="map">
		select * from ops_keepalived_point order by interval
	</select>

	<select id="findByKey" resultMap="map" parameterType="string">
		select * from ops_keepalived_point where key=#{key}
	</select>


</mapper> 