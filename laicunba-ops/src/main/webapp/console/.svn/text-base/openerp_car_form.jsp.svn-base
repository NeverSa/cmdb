<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<script type="text/javascript" src="../js/jquery-1.11.0.js"></script>
<script type="text/javascript" src="openerp_car_form.js?v=1"></script>
<style type="text/css">
#formDiv {
	width: 100%;
	padding: 10px;
	position: fixed;
	bottom: 0px;
	margin-bottom: 30px;
}

body {
	padding: 0px;
	margin: 0px;
}

.span {
	font-size: 30px;
	line-height: 30px;
	color: white;
}

.input {
	height: 30px;
	width: 100px;
	font-size: 30px;
	line-height: 30px;
	border: 0px;
	border-left: 0px solid gray;
}

.input-full {
	width: 99%;
	height: 99%;
	border: 0px;
}

.input-current {
	background-color: #ccc;
}

td, th {
	border: 1px solid gray;
	border-top: 0px;
	border-right: 0px;
}

#formTable td {
	height: 30px;
	padding: 1px
}

#btnSave{ 
	width: 100px;
	margin-top: 10px;
}
.require{
	background-color: red;
}
</style>
</head>
<body>
	<table style="width: 100%;" border="0" id="formTable" cellspacing=0
		cellpadding=0>
		<thead>
			<tr>
				<th class="require">日期</th>
				<th class="require">车辆</th>
				<th class="require">司机1</th>
				<th>司机2</th>
				<th>司机2</th>
				<th class="require">起点</th>
				<th class="require">终点</th>
				<th>单价</th>
				<th>次数</th>
			</tr>
		</thead>
		<tbody id="tbody">

		</tbody>
	</table>
	<button id="btnSave">保存</button>
	<button id="btnCopy">复制上一行</button>
	<table id="template" style="display: none;">
		<tbody>
			<tr>
				<td><input col="1" class="input-full"></td>
				<td><input col="2" class="input-full"></td>
				<td><input col="3" class="input-full"></td>
				<td><input col="4" class="input-full"></td>
				<td><input col="5" class="input-full"></td>
				<td><input col="6" class="input-full"></td>
				<td><input col="7" class="input-full"></td>
				<td><input col="8" class="input-full"></td>
				<td><input col="9" class="input-full"></td>
			</tr>
		</tbody>
	</table>
	<ul id="selectComm">
	
	</ul>
</body>
</html>