<%= simple_form_for(@xiaoxiliebiao) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :yonghuxingming,label:"用户姓名" %>
    <%= f.input :shoujihao,label:"手机号" %>
    <%= f.input :xiaoxileixing,label:"消息类型" %>
    <%= f.input :chuangjianshijian,label:"创建时间" %>
    <%= f.input :biaoti,label:"标题" %>
    <%= f.input :neirong,label:"内容" %>
    <%= f.input :caozuo,label:"操作" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
