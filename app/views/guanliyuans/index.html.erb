<h1>guanliyuan</h1>

<table>
  <thead>
    <tr>
      <th>账号</th>
      <th>姓名</th>
      <th>创建时间</th>
      <th>等级</th>
      <th>suoshujiaose</th>
      <th>所属机构</th>
      <th>性别</th>
      <th>手机</th>
      <th>启用</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @guanliyuans.each do |guanliyuan| %>
      <tr>
        <td><%= guanliyuan.zhanghao %></td>
        <td><%= guanliyuan.xingming %></td>
        <td><%= guanliyuan.chuangjianshijian %></td>
        <td><%= guanliyuan.dengji %></td>
        <td><%= guanliyuan.suoshujiaose %></td>
        <td><%= guanliyuan.suoshujigou %></td>
        <td><%= guanliyuan.xingbie %></td>
        <td><%= guanliyuan.shouji %></td>
        <td><%= guanliyuan.qiyong %></td>
        <td><%= link_to 'Show', guanliyuan %></td>
        <td><%= link_to 'Edit', edit_guanliyuan_path(guanliyuan) %></td>
        <td><%= link_to 'Destroy', guanliyuan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

